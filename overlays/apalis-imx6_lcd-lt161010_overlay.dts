// Enable the parallel RGB interface with setting for 7 inch Displays on the Apalis iMX6Q

/dts-v1/;
/plugin/;

/ {
	compatible = "toradex,apalis_imx6q";
};

&backlight {
	status = "okay";
};

&lcd {
	status = "okay";
};

&mxcfb3 {
	mode_str ="FusionF07A";
	status = "okay";
};
