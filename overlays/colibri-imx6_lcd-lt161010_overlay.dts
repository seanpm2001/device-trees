// Turn on the parallel RGB interface with setting for 7 inch Displays on the Colibri iMX6DL

/dts-v1/;
/plugin/;

/ {
	compatible = "toradex,colibri_imx6dl";
};

&backlight {
	status = "okay";
};

&lcd {
	status = "okay";
};

&mxcfb1 {
	mode_str ="FusionF07A";
	status = "okay";
};

&pwm3 {
	status = "okay";
};
